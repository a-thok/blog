{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "linebreak-style": 0,
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "objects": "always-multiline",
      "imports": "always-multiline",
      "exports": "always-multiline",
      "functions": "never"
    }],
    "react/prop-types": 0,
    "react/no-danger": 0,
    "react/sort-comp": 0,
    "react/no-did-mount-set-state": 0
  }
}
