{
	"compilerOptions": {
    "allowJs": true,
    "checkJs": true,
		"allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "jsxFactory": "Inferno.createVNode",
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
    "node_modules",
    "src/sw.js"
	]
}
